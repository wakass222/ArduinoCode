#define NUMIMGS 7

const unsigned char *images[] = { image0, 0,
                                  image1, 0,
                                  image2, image2b,
                                  image3, image3b,
                                  image4, image4b,
                                  image5, 0,
                                  image6, 0,
                                  image7, 0 };



void setup() {
  delay(30); // initial delay of a few seconds is recommended
  FastLED.addLeds<WS2812, DATA_PIN, GRB>(leds, NUM_LEDS);
  FastLED.setBrightness(BRIGHTNESS);// global brightness
  
  /* Switches for the remote control */
  pinMode( 2,INPUT_PULLUP);
  pinMode( 3,INPUT_PULLUP);
}
 

// main program
void loop() {
  int i,a,b,x;
  char red,green,blue;
  b=0;
  a=0;
  
  while ( true ) {
    /* Display an image */
    x=0;
    for ( i=0 ; i < NUM_LEDS ; i++) {
      red = pgm_read_byte(&(images[a+b][x++]));
      green = pgm_read_byte(&(images[a+b][x++]));
      blue = pgm_read_byte(&(images[a+b][x++]));
      leds[i] = CRGB(red, green, blue);
    }
    FastLED.show();
    delay (100);

    /* Switch handling */

    while ( digitalRead(2) == 1 && digitalRead(3) == 1)
      delay(10);

    if (digitalRead(2) == 0) {
      a+=2;
      b=0;
      
      if ( a > (NUMIMGS * 2))
        a=0;
    }

    if (digitalRead(3) == 0)
      if ( images[a+1] != 0)
        b^=1;

    x=0;
    while ( digitalRead(2) != 1 || digitalRead(3) != 1) {
      delay(10);
      x++;
      
      /* Long press handling */
      if ( x == 100 ) {
        for ( i=0 ; i < NUM_LEDS ; i++) {
          leds[i] = CRGB(0, 0, 0);
        }
        FastLED.show();
        while ( digitalRead(2) != 1 || digitalRead(3) != 1)
          delay(10);
        delay(50);
        while ( digitalRead(2) == 1 && digitalRead(3) == 1)
          delay(10);
      }
    }
  }
}
